{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "allowJs": true,
    "outDir": "./JS",
    "moduleResolution": "Node",
    "downlevelIteration": true
  },
  "include": [
    "./Programmers/*.ts",
    "./Baekjoon/*.ts",
    "./HackerRank/*.ts",
    "./LeetCode/*.ts",
    "./Toy Problem/*.ts"
  ],
  "exclude": ["node_modules", "JS"]
}
